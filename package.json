  "scripts": {
    "@vx/ci/format:check": "pnpm format:check",
    "format": "bash v/bin/cli-format.sh",
    "format:check": "prettier . --check --cache && pnpm format:pkg-check",
    "format:lint:eslint": "eslint --cache --cache-strategy metadata",
    "format:lint:eslint-hard": "eslint",
    "format:lint:stylelint": "stylelint \"**/*.{css,less,module.less}\" --allow-empty-input --cache --cache-strategy metadata",
    "format:pkg": "sort-package-json \"package.json\" \"**/**/__mocks__/package.json\"  \"packages/**/package.json\" \"packages/**/**/package.json\" --quiet",
    "format:pkg-check": "sort-package-json \"package.json\" \"**/**/__mocks__/package.json\"  \"packages/**/package.json\" \"packages/**/**/package.json\"",
    "format:prettier": "prettier . --write --loglevel silent --cache --cache-strategy metadata",
    "format:prettier-hard": "prettier . --write",
    "nx:graph": "nx graph",
    "nx:migrate:chk": "nx migrate latest",
    "nx:migrate:run": "pnpm install --no-frozen-lockfile && nx migrate --run-migrations",
    "tw:migrate": "npx @tailwindcss/upgrade"
  },
